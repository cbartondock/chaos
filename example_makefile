#replace the .dylibs with the files you want to make, and replace /usr/local/bin/gcc-5 with your version of gcc. 


all: ergodic.dylib  invariant.dylib invariant4.dylib quasiperiodicity.dylib switch.dylib sticky.dylib 
exe: ergodic invariant invariant4 quasiperiodicity switch sticky

% : %.c 
	/usr/local/bin/gcc-5 -O3 -Wall -std=c99 -o $@ $^ 

%.o : %.c
	/usr/local/bin/gcc-5 -O3 -Wall -std=c99 -fPIC -c $^

%.dylib : %.o
	/usr/local/bin/gcc-5 -Wall -dynamiclib -o $@ $^

%.so: %.o
	/usr/local/bin/gcc-5 -Wall -shared -o $@ $^

clean:
	rm -f *.so *.o *.dylib

